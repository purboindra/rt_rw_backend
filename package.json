  "main": "index.js",
  "scripts": {
    "dev": "bun --watch --env-file=.env.local src/index.ts",
    "build": "tsc -p tsconfig.json",
    "db:dev:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:stg:migrate": "dotenv -e .env.staging -- prisma migrate dev",
    "db:dev:studio": "dotenv -e .env.local -- prisma studio",
    "db:stg:deploy": "dotenv -e .env.staging -- prisma migrate deploy",
    "db:local:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "db:stg:pull": "dotenv -e .env.staging -- prisma db pull",
    "db:stg:studio": "dotenv -e .env.staging -- prisma studio",
    "start:staging": "bun --env-file=.env.staging dist/index.js",
    "worker:reports": "bun --env-file=.env.local src/workers/reportCreated.worker.ts",
    "worker:outbox": "LOG_LEVEL=debug bun --env-file=.env.local src/workers/outbox.worker.ts"
  },
