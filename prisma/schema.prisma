generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String
  phone     String   @unique
  email     String?  @unique
  role      Role     @default(WARGA)
  address   String
  createdAt DateTime @default(now())
  
}

enum Role {
  ADMIN
  WARGA
  PENGURUS
}